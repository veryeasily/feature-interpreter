{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 14",
 
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  }  
}
